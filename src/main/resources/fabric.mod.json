{
  "schemaVersion": 1,
  "id": "nub-lib",
  "version": "${version}",
  "name": "NUB Lib",
  "description": "Library with common functionality between my mods",
  "authors": [
    "NUB31"
  ],
  "contact": {
    "homepage": "https://oliste.no/",
    "sources": "https://github.com/NUB31/nub-lib"
  },
  "license": "MIT",
  "icon": "assets/nub-lib/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.nublib.NubLib"
    ],
    "client": [
      "com.nublib.NubLibClient"
    ],
    "modmenu": [
      "com.nublib.modmenu.ModMenu"
    ]
  },
  "mixins": [
    "nub-lib.mixins.json",
    {
      "config": "nub-lib.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.15.7",
    "minecraft": "~1.20.4",
    "java": ">=17",
    "fabric-api": "*"
  },
  "suggests": {
    "modmenu": "*"
  }
}